{
  "name": "@fxhash/fxhash-package",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "lerna clean && rm -rf ./node_modules",
    "build": "lerna run build",
    "build:watch": "lerna watch -- lerna run build --scope=\\$LERNA_PACKAGE_NAME",
    "test:watch": "lerna watch -- echo \\$LERNA_PACKAGE_NAME \\$LERNA_FILE_CHANGES"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "lerna": "^7.1.1",
    "nx": "^15.9.2",
    "prettier": "^2.8.7",
    "tsc-alias": "^1.8.5",
    "typescript": "^4.9.5"
  }
}
