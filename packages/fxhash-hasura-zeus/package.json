{
  "name": "@fxhash/hasura-zeus",
  "version": "0.0.1",
  "description": "Hasura GQL API client for FXHASH",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "gen-client": "zeus http://localhost:8888/v1/graphql ./ --typescript --typedDocumentNode --header 'x-hasura-admin-secret: changeme'",
    "test": "jest"
  },
  "author": "fxhash",
  "devDependencies": {
    "@genql/cli": "6.2.1",
    "@graphql-codegen/typed-document-node": "5.0.1",
    "@graphql-typed-document-node/core": "3.2.0",
    "@jest/globals": "29.6.4",
    "graphql-ws": "5.14.0",
    "graphql-zeus": "5.3.2",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "tsup": "6.6.0",
    "ws": "8.14.2"
  },
  "dependencies": {
    "graphql-tag": "2.12.6"
  }
}